<h1>Listing Stocks</h1>

<%= link_to 'New stock', new_stock_path %>

<table>
  <tr>
    <th>Stock Number</th>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @stock.each do |stock| %>
      <tr>
        <td><%= stock.stockNumber %></td>
        <td><%= stock.title %></td>
        <td><%= stock.text %></td>
      </tr>
  <% end %>
</table>